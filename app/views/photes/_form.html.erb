<%#= form_for phote do |f| %>

<%= form_with model: @phote, url: photes_path, method: :post do |f| %>
  <div class="form-group">
    <label>Phote Title</label>
    <%= f.text_field :title, class: "form-control" %>
  </div>

  <div class="form-group">
    <label>Diary</label>
    <%= f.text_area :body, class: "form-control" %>
  </div>

  <div>
    <%= f.submit class: "btn btn-success" %>
  </div>
<% end %>